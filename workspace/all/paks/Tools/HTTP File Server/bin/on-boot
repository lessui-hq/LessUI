#!/bin/sh
BIN_DIR="$(dirname "$0")"
PAK_DIR="$(dirname "$BIN_DIR")"
PAK_NAME="$(basename "$PAK_DIR")"
PAK_NAME="${PAK_NAME%.*}"
set -x

rm -f "$LOGS_PATH/$PAK_NAME.on-boot.log"
exec >>"$LOGS_PATH/$PAK_NAME.on-boot.log" 2>&1

echo "$0" "$@"
cd "$PAK_DIR" || exit 1

main() {
    "$BIN_DIR/service-on" &
}

main "$@"
